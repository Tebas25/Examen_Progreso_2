<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AlmeidaSebastianExamenProgreso2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AlmeidaSebastianExamenProgreso2"
    Shell.FlyoutBehavior="Flyout"
    Shell.FlyoutWidth="250"
    FlyoutHeaderBehavior="CollapseOnScroll"
    Title="AlmeidaSebastianExamenProgreso2">

    <FlyoutItem Title="Recargas">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}"/>
    </FlyoutItem>
    <FlyoutItem Title="GrindLayout">
        <ShellContent ContentTemplate="{DataTemplate local:Salmeida_GridLayout}"/>
    </FlyoutItem>

</Shell>
